    "symfony/config": "^7.2",
    "symfony/filesystem": "^7.2",
    "symfony/polyfill-ctype": "^1.31",

    "psr/log": "~1.0",
    "psr/http-message": "~1.0",

    "google/protobuf": "3.7.1",
    "grpc/grpc": "1.19.0"
