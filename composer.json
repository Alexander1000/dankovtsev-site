  "minimum-stability": "dev",
  "require": {
    "php": ">=8.0",
    "ext-yaml": "*",
    "phing/phing": "^2.16",
    "twig/twig": "^3.19",

    "alexander1000/service-session-client": "0.1.0",
    "alexander1000/service-users-client": "^0.1",
    "alexander1000/service-auth-client": "^0.0",

    "symfony/dependency-injection": "^7.2",
